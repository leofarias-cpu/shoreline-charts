{
  "name": "@vtex/shoreline-design-tokens",
  "version": "0.0.0",
  "description": "Shoreline design tokens",
  "main": "dist/vtex-shoreline-design-tokens.cjs.js",
  "module": "dist/vtex-shoreline-design-tokens.esm.js",
  "types": "dist/vtex-shoreline-design-tokens.cjs.d.ts",
  "repository": "vtex/shoreline",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "codegen": "yarn shorelinecss codegen",
    "build": "npm run build:tokens && ts-node --project tsconfig.node.json scripts/build.ts",
    "build:types": "ts-node --project tsconfig.node.json scripts/build-types.ts",
    "build:tokens": "ts-node --project tsconfig.node.json scripts/build-tokens.ts"
  },
  "devDependencies": {
    "@types/cssesc": "3.0.0",
    "@types/fs-extra": "11.0.1",
    "@types/node": "20.4.5",
    "cssesc": "3.0.0",
    "fs-extra": "11.1.1",
    "lightningcss": "1.21.5",
    "ts-node": "10.9.1"
  },
  "dependencies": {
    "@vtex/shoreline-utils": "0.0.0",
    "@vtex/shoreline-seashell": "0.0.0"
  }
}
