{
  "name": "@vtex/shoreline-components",
  "version": "0.3.0",
  "main": "dist/vtex-shoreline-components.cjs.js",
  "module": "dist/vtex-shoreline-components.esm.js",
  "types": "dist/vtex-shoreline-components.cjs.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/vtex-shoreline-components.cjs.js",
      "import": "./dist/vtex-shoreline-components.esm.js",
      "types": "./dist/vtex-shoreline-components.cjs.d.ts"
    },
    "./styles": "./dist/styles.min.css"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prebuild": "rm -rf dist && rm -rf shoreline && pnpm run theme",
    "dev": "rollup --config rollup.config.js -w --bundleConfigAsCjs",
    "build": "npm run prebuild && rollup --config rollup.config.js --bundleConfigAsCjs && npm run build:css",
    "build:css": "lightningcss --minify --bundle --targets '>= 0.25%' ./src/styles.css -o ./dist/styles.min.css",
    "theme": "shorelinecss theme"
  },
  "repository": {
    "directory": "packages/admin-ui",
    "type": "git",
    "url": "git+https://github.com/vtex/admin-ui.git"
  },
  "bugs": {
    "url": "https://github.com/vtex/admin-ui/issues"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@vanilla-extract/css": "1.13.0",
    "@vanilla-extract/rollup-plugin": "1.3.0",
    "@vtex/shoreline-cli": "workspace",
    "@vtex/shoreline-preset-admin": "workspace",
    "@vtex/shoreline-theme": "workspace",
    "@vtex/shoreline-vanilla-extract": "^0.1.0",
    "lightningcss-cli": "1.22.0",
    "@vtex/shoreline-utils": "^0.0.1-dev.0"
  },
  "dependencies": {
    "@ariakit/react": "0.3.3",
    "@vtex/shoreline-icons": "^0.1.0",
    "@vtex/shoreline-utils": "^0.1.0"
  }
}
