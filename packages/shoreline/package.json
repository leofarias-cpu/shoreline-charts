{
  "name": "@vtex/shoreline",
  "version": "1.0.0-rc.28",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./css": "./dist/styles.css",
    "./css-unlayered": "./dist/styles-unlayered.css"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "dev": "concurrently \"tsup --watch\" \"pnpm run dev:css\" \"pnpm run dev:css-unlayered\"",
    "dev:css": "postcss ./src/styles.css -o dist/styles.css -w",
    "dev:css-unlayered": "postcss ./src/styles-unlayered.css -o dist/styles-unlayered.css -w",
    "build": "concurrently \"tsup\" \"pnpm run build:css\" \"pnpm run build:css-unlayered\"",
    "build:css": "postcss ./src/styles.css -o dist/styles.css",
    "build:css-unlayered": "postcss ./src/styles-unlayered.css -o dist/styles-unlayered.css"
  },
  "repository": {
    "directory": "packages/shoreline",
    "type": "git",
    "url": "git+https://github.com/vtex/shoreline.git"
  },
  "bugs": {
    "url": "https://github.com/vtex/shoreline/issues"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/node": "20.11.6",
    "concurrently": "8.2.2",
    "cssnano": "6.0.2",
    "postcss": "8.4.32",
    "postcss-cli": "11.0.0",
    "postcss-import": "15.1.0",
    "postcss-preset-env": "9.3.0"
  },
  "dependencies": {
    "@vtex/shoreline-components": "^1.0.0-rc.28",
    "@vtex/shoreline-css": "^1.0.0-rc.28",
    "@vtex/shoreline-date": "^1.0.0-rc.28",
    "@vtex/shoreline-icons": "^1.0.0-rc.28",
    "@vtex/shoreline-primitives": "^1.0.0-rc.28",
    "@vtex/shoreline-theme-sunrise": "^1.0.0-rc.28",
    "@vtex/shoreline-utils": "^1.0.0-rc.28"
  }
}
