{
  "name": "@vtex/shoreline-css-engine",
  "version": "0.0.0",
  "description": "Shoreline css engine",
  "main": "dist/vtex-shoreline-css-engine.cjs.js",
  "module": "dist/vtex-shoreline-css-engine.esm.js",
  "types": "dist/vtex-shoreline-css-engine.cjs.d.ts",
  "repository": "vtex/shoreline",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "dev": "rollup --config rollup.config.js -w --bundleConfigAsCjs",
    "build": "npm run prebuild && rollup --config rollup.config.js --bundleConfigAsCjs"
  },
  "dependencies": {
    "@types/cssesc": "3.0.0",
    "@vtex/shoreline-utils": "0.0.0",
    "cssesc": "3.0.0",
    "lightningcss": "1.21.5"
  }
}
