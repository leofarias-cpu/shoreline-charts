{
  "name": "@vtex/shoreline-stitches",
  "version": "0.2.0",
  "description": "Shoreline stitches integration",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": "vtex/shoreline",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "dev": "tsup --watch",
    "build": "npm run prebuild && tsup"
  },
  "dependencies": {
    "@stitches/react": "1.2.8",
    "@vtex/shoreline-csx": "^0.2.0",
    "@vtex/shoreline-utils": "^0.5.0"
  }
}
