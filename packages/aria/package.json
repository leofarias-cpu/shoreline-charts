{
  "name": "@vtex/shoreline-aria",
  "version": "0.0.0",
  "description": "Headless components focused on performance and A11y",
  "main": "dist/vtex-shoreline-aria.cjs.js",
  "module": "dist/vtex-shoreline-aria.esm.js",
  "types": "dist/vtex-shoreline-aria.cjs.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "dev": "rollup --config rollup.config.js -w --bundleConfigAsCjs",
    "build": "npm run prebuild && rollup --config rollup.config.js --bundleConfigAsCjs"
  },
  "repository": {
    "directory": "packages/aria",
    "type": "git",
    "url": "git+https://github.com/vtex/shoreline.git"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "postcss": "8.4.27",
    "postcss-preset-env": "9.1.1",
    "rollup-plugin-postcss": "4.0.2"
  },
  "dependencies": {
    "@ariakit/react": "0.2.15"
  }
}
