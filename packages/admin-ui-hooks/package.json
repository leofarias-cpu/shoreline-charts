{
  "name": "@vtex/admin-ui-hooks",
  "description": "admin-ui shared hooks",
  "license": "MIT",
  "version": "0.2.2",
  "main": "dist/index.js",
  "module": "dist/admin-ui-hooks.esm.js",
  "typings": "dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "directory": "packages/hooks",
    "type": "git",
    "url": "git+https://github.com/vtex/admin-ui.git"
  },
  "bugs": {
