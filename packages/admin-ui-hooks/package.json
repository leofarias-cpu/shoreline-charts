    "build:fast": "tsdx build --tsconfig tsconfig.json --transpileOnly --format esm,cjs",
    "test": "tsdx test",
    "test:ci": "tsdx test --silent"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "dependencies": {
    "@vtex/admin-ui-util": "^0.2.2",
    "use-debounce": "^7.0.0"
