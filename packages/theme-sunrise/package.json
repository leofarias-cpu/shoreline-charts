{
  "name": "@vtex/shoreline-theme-sunrise",
  "version": "1.0.0-rc.25",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./css": "./dist/sunrise.css",
    "./css-unlayered": "./dist/sunrise-unlayered.css"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prebuild": "rm -rf dist && rm -rf shoreline && pnpm run theme",
    "dev": "concurrently -p [{name}] -n css@shoreline,css-unlayered@shoreline -c bgGreen,bgBlue \"pnpm run dev:css\" \"pnpm run dev:css-unlayered\"",
    "dev:css": "postcss ./src/index.scss -o dist/sunrise.css -w",
    "dev:css-unlayered": "postcss ./src/index-unlayered.scss -o dist/sunrise-unlayered.css -w",
    "build": "pnpm run prebuild && pnpm run build:css && pnpm run build:css-unlayered && pnpm run build:ts",
    "build:ts": "tsup",
    "build:css": "postcss ./src/index.scss -o dist/sunrise.css",
    "build:css-unlayered": "postcss ./src/index-unlayered.scss -o dist/sunrise-unlayered.css",
    "theme": "shoreline css"
  },
  "repository": {
    "directory": "packages/theme-sunrise",
    "type": "git",
    "url": "git+https://github.com/vtex/shoreline.git"
  },
  "bugs": {
    "url": "https://github.com/vtex/shoreline/issues"
  },
  "dependencies": {
    "@vtex/shoreline-utils": "^1.0.0-rc.25"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "5.1.1",
    "@types/node": "20.11.6",
    "@vtex/shoreline-cli": "workspace",
    "@vtex/shoreline-css": "workspace",
    "concurrently": "8.2.2",
    "cssnano": "6.0.2",
    "postcss": "8.4.32",
    "postcss-cli": "11.0.0",
    "postcss-import": "15.1.0",
    "postcss-preset-env": "9.3.0",
    "postcss-scss": "4.0.9"
  }
}
