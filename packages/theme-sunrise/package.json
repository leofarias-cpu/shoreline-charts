{
  "name": "@vtex/shoreline-theme-sunrise",
  "version": "1.0.0-rc.22",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./css": "./dist/sunrise.css",
    "./css-tokens": "./dist/tokens.css",
    "./css-components": "./dist/components.css"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prebuild": "rm -rf dist && rm -rf shoreline && pnpm run theme",
    "dev": "concurrently -p [{name}] -n styles:all@shoreline,styles:tokens@shoreline,styles:components@shoreline -c bgGreen,bgBlue,bgPink \"pnpm run dev:all\" \"pnpm run dev:tokens\" \"pnpm run dev:components\"",
    "dev:all": "postcss ./src/index.scss -o dist/sunrise.css -w",
    "dev:tokens": "postcss ./src/tokens/index.scss -o dist/tokens.css -w",
    "dev:components": "postcss ./src/components/index.scss -o dist/components.css -w",
    "build": "pnpm run prebuild && pnpm run build:all && pnpm run build:tokens && pnpm run build:components && pnpm run build:ts",
    "build:ts": "tsup",
    "build:all": "postcss ./src/index.scss -o dist/sunrise.css",
    "build:tokens": "postcss ./src/tokens/index.scss -o dist/tokens.css",
    "build:components": "postcss ./src/components/index.scss -o dist/components.css",
    "theme": "shoreline css"
  },
  "repository": {
    "directory": "packages/theme-sunrise",
    "type": "git",
    "url": "git+https://github.com/vtex/shoreline.git"
  },
  "bugs": {
    "url": "https://github.com/vtex/shoreline/issues"
  },
  "dependencies": {
    "@vtex/shoreline-utils": "^1.0.0-rc.22"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "5.1.1",
    "@types/node": "20.11.6",
    "@vtex/shoreline-cli": "workspace",
    "@vtex/shoreline-css": "workspace",
    "concurrently": "8.2.2",
    "cssnano": "6.0.2",
    "postcss": "8.4.32",
    "postcss-cli": "11.0.0",
    "postcss-import": "15.1.0",
    "postcss-preset-env": "9.3.0",
    "postcss-scss": "4.0.9"
  }
}
