{
  "name": "onda",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtex/admin-ui.git"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lerna": "lerna",
    "turbo": "turbo",
    "build": "pnpm turbo run build",
    "dev": "pnpm turbo run dev --no-cache --parallel --continue",
    "test": "jest --color --runInBand --ci",
    "version": "lerna run version",
    "prepublish:main": "pnpm install && pnpm build",
    "prepublish:dev": "pnpm install && pnpm build",
    "publish:main": "pnpm lerna publish",
    "publish:dev": "pnpm lerna publish prerelease --preid dev --dist-tag dev",
    "clean": "pnpm turbo clean && turbo run clean",
    "format": "prettier --write \"packages/**/*.{ts,js,tsx,jsx,json}\"",
    "lint": "eslint packages/ --ext .js,.ts,.jsx,.tsx",
    "build-storybook": "storybook build",
    "dev-storybook": "pnpm storybook dev -p 6006",
    "build:storybook": "pnpm build && pnpm storybook build",
    "chromatic": "chromatic --exit-zero-on-changes --build-script-name=build:storybook",
    "docs": "npm --prefix packages/admin-ui-docs run start",
    "next-docs": "npm --prefix packages/next-docs run dev",
    "build:docs": "npm --prefix packages/admin-ui-docs run build-docs",
    "build:next-docs": "npm --prefix packages/next-docs run build-docs",
    "version:docs": "npm --prefix packages/admin-ui-docs run docusaurus docs:version",
    "commit": "cz",
    "build:ci": "pnpm turbo run build",
    "build-storybook:ci": "pnpm storybook build",
    "test:ci": "jest --color --runInBand --ci",
    "lint:ci": "pnpm lint"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@babel/cli": "7.22.5",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/plugin-transform-runtime": "7.22.5",
    "@babel/preset-env": "7.22.5",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@babel/runtime": "7.22.5",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@commitlint/prompt": "^9.1.2",
    "@jest/types": "27.4.2",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "25.0.2",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-typescript": "11.1.1",
    "@storybook/addon-a11y": "^7.2.1",
    "@storybook/addon-actions": "^7.2.1",
    "@storybook/addon-essentials": "^7.2.1",
    "@storybook/addon-links": "^7.2.1",
    "@storybook/react": "^7.2.1",
    "@storybook/theming": "7.2.1",
    "@storybook/react-webpack5": "^7.2.1",
    "@storybook/react-vite": "7.2.1",
    "vite": "4.3.9",
    "@swc-node/jest": "^1.3.5",
    "@swc/core": "^1.2.177",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "^27.4.1",
    "@types/jest-axe": "^3.5.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/rollup-plugin-peer-deps-external": "2.2.1",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/parser": "5.60.1",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@vtex/prettier-config": "^0.3.1",
    "@vtex/tsconfig": "^0.5.6",
    "babel-loader": "9.1.2",
    "babel-plugin-macros": "3.1.0",
    "babel-plugin-module-resolver": "5.0.0",
    "babel-plugin-typescript-to-proptypes": "2.1.0",
    "chromatic": "^6.4.3",
    "commitizen": "^4.2.0",
    "eslint": "8.43.0",
    "eslint-config-vtex": "15.0.2",
    "eslint-config-vtex-react": "9.0.3",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-tsc": "2.0.0",
    "husky": "^4.2.5",
    "jest": "^27.5.1",
    "jest-axe": "^4.0.0",
    "jest-environment-jsdom": "^27.3.1",
    "jest-watch-typeahead": "1.0.0",
    "lerna": "7.1.4",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "raw.macro": "^0.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "rollup": "3.25.3",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-dts": "5.3.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-terser": "7.0.2",
    "storybook": "^7.2.1",
    "turbo": "1.4.3",
    "typescript": "4.9.5",
    "url-loader": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "pnpm test",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm lint --fix"
    ],
    "*.{gql,json,yml,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
